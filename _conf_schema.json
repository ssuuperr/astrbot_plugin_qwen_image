{
    "api_url": {
    "description": "API 基础地址",
    "type": "string",
    "default": "https://api.siliconflow.cn",
    "hint": "API服务器的基础地址"
    },

    "api_key": {
        "description": "API 密钥",
        "type": "string",
        "hint": "请前往 https://cloud.siliconflow.cn/ 注册账号并获取 API Key",
        "default": "API_Key",
        "obvious_hint": true
    },
    "model": {
        "description": "使用的模型名称",
        "type": "string",
        "default": "Qwen/Qwen-Image",
        "hint": "调用的生图模型的名称"
    },
    "size": {
        "description": "生成图片的分辨率",
        "type": "string",
        "default": "1024x1024",
        "hint": "图片分辨率，格式为 宽x高。具体请查阅模型支持的分辨率。"
    },
    "num_inference_steps": {
        "description": "推理步数",
        "type": "int",
        "default": 20,
        "hint": "生成图片时的推理步数，数值越大生成质量可能变高。"
    },
    "seed": {
        "description": "种子ID",
        "type": "string",
        "default": "随机",
        "hint": "用于控制生成结果的随机性，输入'随机'则自动生成随机种子。"
    }
}
